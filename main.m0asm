; main starting point     
@start:                         ldi gr0 10
                                ldi gr1 5

                                cai rel @mul

                                hlt

; multiplication function
@mul:                           ldi grb 0

.mul_loop:                      add gr2 gr1 gr2
                                dec gr0
                                ne gra gr0 grb
                                jii rel .mul_loop gra

                                ret